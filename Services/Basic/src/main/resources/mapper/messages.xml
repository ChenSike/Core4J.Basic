<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ikcoder.services.mappers.Mapper$messages">

    <select id="Select$message$message" resultType="ikcoder.services.entity.DTO.coredb_basic.DTO$message">
        select * from messages where message=#{message}
    </select>

    <select id="Select$message$code" resultType="ikcoder.services.entity.DTO.coredb_basic.DTO$message">
        select * from messages where code=#{code}
    </select>

</mapper>